<h1>User Discover Index</h1>
<p><%= button_to 'Discover Top Rated Movies', user_movies_path(@user), method: :get, params: {top_rated: true} %></p>
<%= form_with url: user_movies_path(@user), method: :get, local: true do |form| %>
  <%= form.label :search, 'Search' %>
  <%= form.text_field :search %>
  <%= form.submit 'Search by Movie Title' %>
<% end %>
